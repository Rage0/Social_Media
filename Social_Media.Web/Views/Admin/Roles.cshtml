@model IEnumerable<IdentityRole>

@{
    Layout = "_AdminLayout";
    ViewBag.Title = "Admin Roles";
}

@foreach (IdentityRole role in Model)
{
    <div class="my-3">
        <div class="card">
            <div class="card-body d-flex justify-content-between">
                <div>
                    <h5>Role name: @role.Name</h5>
                </div>
                <div>
                    <p class="fs-5">Id: @role.Id</p>
                </div>
            </div>
        </div>
    </div>
}
